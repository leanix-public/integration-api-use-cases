			"relations": {
				"filter": [
					"relApplicationToUserGroup",
					"relToParent"
				],
				"fields": [
					"usageType",
					"activeFrom",
					"activeUntil"
				],
				"targetFields": [
					"name",
					"displayName",
					"externalId"
				]
			},
			"tags": {
				"groups": [
					"Application Type",
					"Kostenstelle",
					"Solution"
				]
			},
			"subscriptions": {
				"types": [
					"ACCOUNTABLE",
					"RESPONSIBLE"
				]
			},
			"output": [
				{
					"key": {
						"expr": "content.id"
					},
					"values": [
						{
							"expr": "${lx.factsheet.id}"
						}
					]
				},
				{
					"key": {
						"expr": "content.type"
					},
					"values": [
						{
							"expr": "${lx.factsheet.type}"
						}
					]
				},
				{
					"key": {
						"expr": "ApplicationName"
					},
					"values": [
						{
							"expr": "${lx.factsheet.name}"
						}
					]
				},
				{
					"key": {
						"expr": "ApplicationDisplayName"
