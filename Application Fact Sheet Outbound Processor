						"expr": "ApplicationParent"
					},
					"mode": "list",
					"values": [
						{
							"forEach": {
								"elementOf": "${lx.relations}",
								"filter": "${integration.output.valueOfForEach.type=='relToParent'}"
							},
							"map": [
								{
									"key": "ApplicationName",
									"value": "${integration.output.valueOfForEach.target.name}"
								},
								{
									"key": "ApplicationDisplayName",
