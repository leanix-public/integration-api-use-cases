					]
				},
				{
					"key": {
						"expr": "ApplicationLifecycle"
					},
					"mode": "list",
					"values": [
						{
							"expr": "plan:${lx.factsheet.lifecycle.plan}",
							"regexMatch": ".*:.+$"
						},
						{
							"expr": "phaseIn:${lx.factsheet.lifecycle.phaseIn}",
							"regexMatch": ".*:.+$"
						},
						{
							"expr": "active:${lx.factsheet.lifecycle.active}",
							"regexMatch": ".*:.+$"
